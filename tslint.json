{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
		"rules": {
			"no-console": false,
			"quotemark": {
				"options": "single"
			},
			"interface-name": {
				"options": "never-prefix"
			},
			"member-access": {
				"options": "no-public"
			},
			"object-literal-sort-keys": false,
			"member-ordering": false,
			"trailing-comma": {
				"options": {
					"multiline": "never",
					"singleline": "never"
				}
			},
			"semicolon": {
				"options": "never"
			},
			"indent": [true, "tabs", 2]
		},
    "rulesDirectory": []
}